local OrionLib = loadstring(game:HttpGet(('https://raw.githubusercontent.com/shlexware/Orion/main/source')))()


local Window = OrionLib:MakeWindow({Name = "ERROR", HidePremium = false, SaveConfig = true, ConfigFolder = "ERROR", IntroIcon = "rbxassetid://15370100403"})


OrionLib:MakeNotification({
    Name = "ERROR!",
    Content = "ERROR",
    Image = "rbxassetid://4483345998",
    Time = 5
})

local ERRORTab = Window:MakeTab({
    Name = "ERROR",
    Icon = "rbxassetid://15370100403",
    PremiumOnly = false
})

ERRORTab:AddLabel("SCRIPT FOI DESATIVADO, SE QUISER UTILIZA-LO, ")
ERRORTab:AddLabel("CONTATE O CRIADOR DO SCRIPT")
ERRORTab:AddLabel("         ")

ERRORTab:AddButton({
    Name = "FECHAR",
    Callback = function()  
        OrionLib:Destroy()
    end    
})
