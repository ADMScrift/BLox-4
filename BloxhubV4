FPSTab:AddButton({
    Name = "Mostrar Fps",
    Callback = function()  ----script logo abaixo:arrow_down::arrow_down::arrow_down:-----
		loadstring(game:HttpGet('https://raw.githubusercontent.com/1201for/littlegui/main/FPS-Counter'))()

 	end    
 })

 
 FPSTab:AddButton({
    Name = "Remover FOG",
    Callback = function()  ----script logo abaixo:arrow_down::arrow_down::arrow_down:-----
		repeat task.wait() until game:IsLoaded()
    wait(5) -- extra load to ensure the script is working
    for i,v in pairs(game.Lighting:GetChildren()) do
	if v:IsA("") or v:IsA("Sky") or v:IsA("BlurEffect") or v:IsA("BloomEffect") or v:IsA("SunRaysEffect") then
	v:Destroy()
	end
	end

    end    
})


