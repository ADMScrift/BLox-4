local FPSTab = Window:MakeTab({
	Name = "Fps",
	Icon = "rbxassetid://4483345998",
	PremiumOnly = false
})


FPSTab:AddButton({
    Name = "Remover Textura",
    Callback = function()  ----script logo abaixo:arrow_down::arrow_down::arrow_down:-----
        for i,v in next, (workspace:GetDescendants()) do
            if v:IsA("Part") then v.Material = Enum.Material.SmoothPlastic
            end
            end
    end    
})

BloxTab:AddButton({
	Name = "HOHO",
	Callback = function()  ----script logo abaixo‚¨áÔ∏è‚¨áÔ∏è‚¨áÔ∏è-----
